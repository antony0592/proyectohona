
@{
    List<ContentPageModel> contentPage = (List<ContentPageModel>)ViewBag.contentPage;
    HotelModel hotel = (HotelModel)ViewBag.hotel;
}


<div class="container">
    <div class="row">
        <div class="col-4 text-center">
            <div>
                <br />

                @foreach (ContentPageModel content in contentPage)
                {
                    <img src=@content.urlimage alt="imagenHotelPatito" />

                }


                <h5 class="titulopagina">@hotel.name</h5>

                <div class="separatorline"></div>
                <h6>HOTEL & RESORT</h6>
            </div>
        </div>
        <div class="col-8">
            <br />
            <div class="w-75">
                <h4 class="titulopagina">Bienvenido al @hotel.name</h4>
                <p>@hotel.description</p>


            </div>
        </div>
    </div>
</div>
